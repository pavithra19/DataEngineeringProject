{
	"name": "testsqlDB_LS",
	"properties": {
		"description": "SQL Database Linked Service",
		"parameters": {
			"databaseName": {
				"type": "string",
				"defaultValue": "testsqlDB"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"server": "testdbserverdev.database.windows.net",
			"database": "@{linkedService().databaseName}",
			"encrypt": "mandatory",
			"trustServerCertificate": false,
			"authenticationType": "SQL",
			"userName": "adminCredentials",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault_LS",
					"type": "LinkedServiceReference"
				},
				"secretName": "SqlDBCredentials"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}
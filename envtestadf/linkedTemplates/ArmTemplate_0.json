					{
						"name": "If Condition Product count",
						"type": "IfCondition",
						"dependsOn": [
							{
								"activity": "Lookup Product count",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greaterOrEquals(activity('Lookup Product count').output.firstRow.RecordCount, 200)",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "Failure message",
									"type": "Fail",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"message": "Product count is not sufficient to perform the copy action.",
										"errorCode": "1001"
									}
								}
							],
							"ifTrueActivities": [
								{
									"name": "Copy child if Product Data",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
